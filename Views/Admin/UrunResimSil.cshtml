
@{
    ViewBag.Title = "Ürün Resim Sil";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
@model IEnumerable<WebUI.Models.Resim>
<br /><br /><br /><br />
<h2>Ürün Resim Sil</h2>
<section class="panel">
    <header class="panel-heading">
        Ürün Resim Sil
    </header>
    <div class="panel-body">
        <div class="adv-table editable-table ">
            <br />
            <br />
            <div class="space15"></div>
            <table class="table table-striped table-hover table-bordered" id="editable-sample">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Yol</th>
                        <th>Açıklama</th>
                        <th>Sil</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var slider in Model)
                    {
                        <tr class="">
                            <td>@slider.Id</td>
                            <td>@slider.OrtaYol</td>
                            <td>@slider.Aciklama</td>
                            <td><a class="delete" href="/Admin/UrunResimSils/@slider.Id"><i class="icon-trash"></i></a></td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</section>


